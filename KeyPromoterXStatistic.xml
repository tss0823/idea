<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'⌘5'</ShortCut>
          <Description>Debug</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'CodeSyncUtils.main()'">
        <Item>
          <IdeaActionID>DebugClass</IdeaActionID>
          <ShortCut>'⌃⇧D'</ShortCut>
          <Description>Debug 'CodeSyncUtils.main()'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'zplugin'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⌘F11'</ShortCut>
          <Description>Debug 'zplugin'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⌘M'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Source">
        <Item>
          <IdeaActionID>EditSource</IdeaActionID>
          <ShortCut>'F3'</ShortCut>
          <Description>Jump to Source</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste">
        <Item>
          <IdeaActionID>$Paste</IdeaActionID>
          <ShortCut>'⌘V'</ShortCut>
          <Description>Paste</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘1'</ShortCut>
          <Description>Project</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'F8'</ShortCut>
          <Description>Resume Program</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Dependencies Popup...">
        <Item>
          <IdeaActionID>ShowUmlDiagramPopup</IdeaActionID>
          <ShortCut>'⌥⌘U'</ShortCut>
          <Description>Show Dependencies Popup...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Dependencies...">
        <Item>
          <IdeaActionID>ShowUmlDiagram</IdeaActionID>
          <ShortCut>'⌥⇧⌘U'</ShortCut>
          <Description>Show Dependencies...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌘6'</ShortCut>
          <Description>Terminal</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⇧⌘B'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'⌘9' or '⇧⌘9'</ShortCut>
          <Description>Version Control</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>